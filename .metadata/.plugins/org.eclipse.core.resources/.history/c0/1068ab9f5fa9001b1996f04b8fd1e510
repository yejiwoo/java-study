package sec02.p347;

public class RemoteControlExample {

	public static void main(String[] args) {

		Television tv=new Television(); //인터페이스 사용하지 x
		
		RemoteControl rc;
		
		//인터페이스 구현 객체 사용 방법
		//인터페이스 변수에 구현 객체 대입
		rc=new Television();
		rc.setVolume(5);
		
		rc=new Audio();
		rc.setVolume(10);
	}

}
