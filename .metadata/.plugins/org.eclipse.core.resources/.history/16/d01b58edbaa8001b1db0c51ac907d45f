package sec01;

public class DmbCellPhoneExample extends CellPhone{
	//field
	private int channel;

	//constructor
	public DmbCellPhoneExample(String model, String color, int channel) {
		// super(); //얘는 뭐지?
		this.model=model;
		this.color=color;
		this.channel = channel;
	}

	//method
	void changeCahneelDmb(int channel) {
		this.channel=channel;
		System.out.println("채널 "+channel+"번으로 바꿉니다.");
	}
	

}
